Private Sub Timer1_Timer()


If enemyfire(0).Left + enemyfire(0).Width > Picture1.Left And enemyfire(0).Left < Picture1.Left + Picture1.Width And enemyfire(0).Top < Picture1.Top And enemycraft1.Visible = True Then enemyfire(0).Visible = True
