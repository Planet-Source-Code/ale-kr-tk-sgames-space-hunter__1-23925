If Picture1.Left < 200 Then
Else
Picture1.Left = Picture1.Left - 150
Picture1.Picture = leftt.Picture
End If
End If



If SMERr = "RIGHT" Then
If Picture1.Left + Picture1.Width > Form1.Width - 700 Then
Else
Picture1.Left = Picture1.Left + 150
Picture1.Picture = rightt.Picture
End If
End If


